

DROP TABLE IF EXISTS freeze_account;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE freeze_account (
  user_id varchar(256) NOT NULL,
  otp_type varchar(256) NOT NULL,
  expiry_time datetime NOT NULL,
  freeze_reason varchar(256) NOT NULL,
  is_deleted varchar(45) NOT NULL DEFAULT 'false'
) ;

LOCK TABLES freeze_account WRITE;

INSERT INTO freeze_account VALUES ('123','REGISTER',now(),'Resend_Attempts','false');


CREATE TABLE user_otp (
  user_id varchar(128) NOT NULL,
  mobile_number varchar(20) DEFAULT NULL,
  email varchar(256) DEFAULT NULL,
  otp varchar(10) NOT NULL,
  otp_id` int(11) NOT NULL AUTO_INCREMENT,
  invalid_attempts` int(11) DEFAULT NULL,
  resend_attempts` int(11) DEFAULT NULL,
  threshold_time` datetime DEFAULT NULL,
  expiry_time` datetime NOT NULL,
  notification_type` varchar(128) DEFAULT NULL,
  otp_type` varchar(128) DEFAULT NULL,
  otp_status` varchar(128) NOT NULL,
  created_on` datetime NOT NULL,
  modified_on` datetime DEFAULT NULL,
  client_id` varchar(250) DEFAULT NULL,
  token` varchar(250) DEFAULT NULL,
  PRIMARY KEY (`otp_id`)
);
