<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.snapdeal.opspanel.promotion.dao.AuditAccessDao">

	<insert id="auditClientDetails" parameterType="com.snapdeal.opspanel.promotion.model.AccessAuditEntity">
		INSERT INTO
		access_audit
		(client_id,client_role,operation_mode,merchant_name,corporate_data,instrument_name,operation_time)
		VALUES
		(#{clientEmail},#{clientRole},#{operationMode},#{merchantName},#{corporateData},#{instrumentName},#{operationTime})
	</insert>
</mapper>
