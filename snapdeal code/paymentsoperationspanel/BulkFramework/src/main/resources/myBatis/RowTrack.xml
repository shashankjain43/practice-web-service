<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.snapdeal.bulkprocess.dao.FileTrackerDao">

	<resultMap id="rowResult"
		type="com.snapdeal.bulkprocess.entity.RowTrackEntity">
		<result property="fileId" column="fileId" />
		<result property="status" column="status" />
		<result property="rowId" column="rowId" />
		<result property="time" column="time" />
		<result property="userId" column="userId" />
	</resultMap>

	<select id="getIncompleteFileIds" resultType="java.lang.Long"
		parameterType="java.lang.String">
		SELECT count(*)from rowtrack
		where fileId=#{fileId}
	</select>

	<select id="getAllRowsForFile" resultMap=""
		parameterType="java.lang.String">
		SELECT * from rowtrack
		where fileId=#{fileId}
	</select>



</mapper>