<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.snapdeal.opspanel.promotion.dao.UsersDao">

	<resultMap id="result" type="com.snapdeal.opspanel.promotion.entity.UsersEntity">
		<result property="email_id" column="email_id" />
		<result property="user_role" column="user_role" />				
	</resultMap> 
	
	<sql id="base_column_list">
		email_id,user_role
	</sql>
	
		<select id="getEntity" resultMap="result" parameterType="java.lang.String" >
		select * from users
		where email_id = #{email_id}
		
	</select>
	</mapper>